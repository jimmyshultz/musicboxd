{
  "name": "Resonare",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "android:dev": "ENVFILE=.env.development react-native run-android --variant=debugDevelopment",
    "android:staging": "ENVFILE=.env.staging react-native run-android --variant=debugStaging",
